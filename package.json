{
  "name": "switchboard-iam",
  "version": "1.2.0-alpha",
  "description": "Decentralized Identity and Access Management Switchboard",
  "author": "EWF",
  "license": "GPL-3.0-or-later",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-dev": "ng build --watch",
    "build-prod": "node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build --configuration production --aot -c=production",
    "build": "node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build --configuration production --aot -c=development",
    "test": "ng test --code-coverage",
    "test:headless": "ng test --karma-config=karma-headless.conf.js",
    "lint": "ng lint",
    "modernizr": "modernizr -c modernizr-config.json -d src/modernizr.js",
    "e2e": "ng e2e",
    "cypress:open": "ng run switchboard-dapp:cypress-open",
    "cypress:run": "ng run switchboard-dapp:cypress-run --spec cypress/integration/ui/**/*.spec.ts",
    "cy:run:ci": "npm run ng serve",
    "precy:run:ci": "echo fs.inotify.max_user_watches=524288 | sudo tee -a /etc/sysctl.conf && sudo sysctl -p"
  },
