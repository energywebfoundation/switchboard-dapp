  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-dev": "ng build --watch",
    "build-prod": "node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build --prod --aot -c=production",
