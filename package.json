{
  "name": "switchboard-iam",
  "version": "1.2.0-alpha",
  "description": "Decentralized Identity and Access Management Switchboard",
  "author": "EWF",
  "license": "GPL-3.0-or-later",
  "private": true,
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-dev": "ng build --watch",
    "setup-sentry": "ts-node ./create-sentry-config.ts",
    "build-staging": "node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build --prod --aot -c=staging",
    "build-prod": "node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build --prod --aot -c=production",
    "build-stedin": "node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build --prod --aot -c=stedin",
    "build": "node --max_old_space_size=8048 node_modules/@angular/cli/bin/ng build --prod --aot -c=development",
    "postinstall": "node patch.js && patch-package",
    "test": "ng test --code-coverage",
    "test:headless:cc": "ng test --karma-config=karma-headless.conf.js --code-coverage",
    "test:headless": "ng test --karma-config=karma-headless.conf.js",
    "lint": "ng lint",
    "modernizr": "modernizr -c modernizr-config.json -d src/modernizr.js",
    "e2e": "ng e2e",
    "cypress:open": "ng run switchboard-dapp:cypress-open",
    "cypress:run": "ng run switchboard-dapp:cypress-run --spec cypress/integration/ui/**/*.spec.ts",
    "cy:run:ci": "npm run ng serve",
    "precy:run:ci": "echo fs.inotify.max_user_watches=524288 | sudo tee -a /etc/sysctl.conf && sudo sysctl -p",
    "stedin:flags": "node scripts/stedin.js"
  },
  "dependencies": {
    "@angular-builders/custom-webpack": "^13.0.0",
    "@angular/animations": "13.1.3",
    "@angular/cdk": "13.1.3",
    "@angular/common": "13.1.3",
    "@angular/compiler": "13.1.3",
    "@angular/core": "13.1.3",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "13.1.3",
    "@angular/material": "13.1.3",
    "@angular/material-moment-adapter": "13.1.3",
    "@angular/platform-browser": "13.1.3",
    "@angular/platform-browser-dynamic": "13.1.3",
    "@angular/router": "13.1.3",
    "@angular/service-worker": "13.1.3",
    "@fortawesome/fontawesome-free": "5.5.0",
    "@gnosis.pm/safe-apps-sdk": "^4.3.0",
    "@mat-datetimepicker/core": "^7.0.1",
    "@mat-datetimepicker/moment": "^7.0.1",
    "@ngrx/effects": "^12.5.0",
    "@ngrx/store": "12.5.0",
    "@ngrx/store-devtools": "12.5.0",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@sentry/angular": "^6.16.1",
    "@zxing/browser": "0.0.9",
    "@zxing/library": "0.18.6",
    "@zxing/ngx-scanner": "^3.2.0",
    "angular-user-idle": "2.2.7",
    "bootstrap": "4.3.1",
    "codemirror": "5.58.2",
    "core-js": "2.5.4",
    "fullcalendar": "3.9.0",
    "hammerjs": "^2.0.8",
    "iam-client-lib": "^4.0.0",
    "modernizr": "3.7.1",
    "moment": "2.22.2",
    "ng-qrcode": "4.3.0",
    "ngx-bootstrap": "^8.0.0",
    "ngx-editor": "9.0.2",
    "ngx-spinner": "^11.0.2",
    "ngx-toastr": "13.2.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "popper.js": "1.14.3",
    "rxjs": "6.6.7",
    "simple-line-icons": "2.4.1",
    "sweetalert": "2.1.2",
    "ts-md5": "^1.2.7",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.1.4",
    "@angular-devkit/core": "13.1.4",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
